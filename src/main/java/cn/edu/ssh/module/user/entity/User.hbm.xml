<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.edu.ssh.module.user.entity">
	<class name="User" table="t_user">
		<id name="id" column="u_id">
			<generator class="increment"></generator>
		</id>
		<property name="userName" column="u_username"></property>
		<property name="name" column="u_name"></property>
		<property name="age" column="u_age"></property>
		<property name="salt" column="salt"></property>
		<property name="regDate" column="reg_date"></property>
		<property name="password" column="u_passwd"></property>
	</class>
</hibernate-mapping>
